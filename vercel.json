{
    "version": 2,
    "buildCommand": "npm run build:vercel",
    "devCommand": "next dev",
    "installCommand": "npm install",
    "framework": "nextjs",
    "images": {
        "sizes": [16, 32, 48, 64, 96, 128, 256, 384, 512],
        "domains": [
            "localhost",
            "raw.githubusercontent.com"
        ],
        "formats": [
            "image/webp",
            "image/avif"
        ]
    },
    "env": {
        "NEXT_DISABLE_ESLINT": "1",
        "NEXT_DISABLE_TYPECHECK": "1"
    }
}